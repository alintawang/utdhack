{
  "manifest_version": 2,
  "name": "LIFEISFREAKINGDUMB",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "jquery-3.3.1.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
     "tabs",
     "*://*/*",
     "<all_urls>",
     "notifications",
     "webRequest",
     "webRequestBlocking"
   ],

  "options_page": "options.html"
}
