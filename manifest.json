{
  "manifest_version": 2,
  "name": "LIFEISFREAKINGDUMB",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup/popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "http://*/*",
    "https://*/*"
  ],

  "options_page": "options/options.html"
}
